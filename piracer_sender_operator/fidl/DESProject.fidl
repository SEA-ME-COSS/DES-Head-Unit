package commonapi


interface IPCManager {
  version {major 1 minor 0}
  method setSensorRpm {
    in {
      UInt16 SensorRpm
    }
    out {
      String message
    }
  }
  method setBatteryLevel {
    in {
      UInt16 BatteryLevel
    }
    out {
      String message
    }
  }
}


interface PiracerOperator {
  version {major 1 minor 0}
  method setGearMode {
    in {
      UInt16 GearMode
    }
    out {
      String message
    }
  }
}

